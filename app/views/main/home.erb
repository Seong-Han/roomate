 $(document).ready(function(){
  $(window).scroll(function(){
    if ($(this).scrollTop() > 520) {
      $('nav').addClass('fixed');
    }
    else {
      $('nav').removeClass('fixed');
    }
  });
});

$(document).ready(function(){
$(document).scroll(function(){
var top=$(this).scrollTop();
if(top<150){
var dif=1-top/150;
$(".header").css({opacity:dif});
}
})
});
	</script>
